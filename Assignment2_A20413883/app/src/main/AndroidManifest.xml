<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mehulsingh.multi_notepad">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/applicationname"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mehulsingh.multinotes.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mehulsingh.multinotes.InfoDataActivity"
            android:parentActivityName="com.mehulsingh.multinotes.MainActivity" />
        <activity
            android:name="com.mehulsingh.multinotes.NoteEditActivity"
            android:parentActivityName="com.mehulsingh.multinotes.MainActivity" />
    </application>

</manifest>